{
  // 有关 tasks.json 格式的文档，请参见
  // https://go.microsoft.com/fwlink/?LinkId=733558
  "version": "2.0.0",
  "tasks": [
    {
      "type": "npm",
      "script": "lint",
      "problemMatcher": ["$eslint-compact"]
    },
    {
      "type": "npm",
      "script": "dev",
      "problemMatcher": ["$eslint-compact"]
    },
    {
      "type": "npm",
      "script": "build",
      "problemMatcher": ["$eslint-compact"],
      "group": {
        "kind": "build",
        "isDefault": true
      }
    },
    {
      "type": "npm",
      "script": "generate",
      "problemMatcher": ["$eslint-compact"]
    },
    {
      "type": "npm",
      "script": "install",
      "problemMatcher": ["$eslint-compact"]
    },
    {
      "type": "npm",
      "script": "start",
      "problemMatcher": ["$eslint-compact"]
    }
  ]
}
